{
  "name": "nettrine/extensions-knplabs",
  "description": "Doctrine2 behavioral extensions for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database", "knplabs", "behaviors"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extensions-knplabs",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/di": "^3.2.2",
    "nette/security": "^3.2.0",
    "doctrine/orm": "^3.0.0",
    "knplabs/doctrine-behaviors": "^3.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "contributte/phpstan": "^0.2.0",
    "nette/http": "^3.3.0",
    "tracy/tracy": "^2.10.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\KnpLabs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
