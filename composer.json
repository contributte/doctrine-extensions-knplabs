{
  "name": "nettrine/extensions-knplabs",
  "description": "Doctrine2 behavioral extensions for Nette Framework",
  "keywords": ["contributte", "nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extensions-knplabs",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/di": "^0.5.0",
    "doctrine/orm": "^2.5.0",
    "knplabs/doctrine-behaviors": "^2.0.8"
  },
  "require-dev": {
    "nette/http": "^3.0.0",
    "nette/security": "^3.0.0",
    "nettrine/orm": "^0.8.1",
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.12.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "tracy/tracy": "^2.7.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\KnpLabs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
